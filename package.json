{
  "name": "lead-survey-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "license": "GPL-3.0-only",
  "scripts": {
    "start": "vite",
    "start:public": "VITE_SURVEY_TYPE=public vite",
    "start:gp": "VITE_SURVEY_TYPE=gp vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "deploy:public:dev": "VITE_SURVEY_TYPE=public yarn build --mode dev && VITE_SURVEY_TYPE=public serverless deploy --stage dev",
    "deploy:gp:dev": "VITE_SURVEY_TYPE=gp yarn build --mode dev && VITE_SURVEY_TYPE=gp serverless deploy --stage dev",
    "deploy:prod": "yarn build --mode prod && srverless deploy --stage prod",
    "deploy:dev": "yarn deploy:public:dev && yarn deploy:gp:dev"
  },
  "dependencies": {
    "@adobe/react-spectrum": "^3.42.2",
    "@babel/runtime": "^7.28.4",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@internationalized/date": "^3.9.0",
    "@mui/icons-material": "^5",
    "@mui/material": "^5.16.7",
    "@react-pdf/renderer": "^3.4.4",
    "@react-spectrum/provider": "^3.10.10",
    "@spectrum-icons/workflow": "^4.2.12",
    "axios": "^1.7.3",
    "bulma": "^1.0.1",
    "dotenv": "^16.4.5",
    "moment": "^2.30.1",
    "react": "^18.2.0",
    "react-aria": "^3.43.2",
    "react-aria-components": "^1.12.2",
    "react-dom": "^18.2.0",
    "react-is": "^19.1.1",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.23.1",
    "remark-gfm": "^4.0.0",
    "serverless": "^4.1.21",
    "serverless-cloudfront-invalidate": "^1.12.2",
    "serverless-s3-sync": "^3.3.0",
    "sharp": "^0.34.4",
    "styled-components": "^6.1.11",
    "ts-node": "^10.9.2",
    "vite-plugin-plain-text": "^1.4.2"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/react-is": "^19",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-basic-ssl": "^2.1.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.57.0",
    "eslint-import-resolver-pnp": "^0.0.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "sass": "^1.81.0",
    "tsx": "^4.19.1",
    "typescript": "^5.2.2",
    "vite": "^6",
    "vite-plugin-mkcert": "^1.17.8"
  },
  "resolutions": {
    "styled-components": "^5"
  },
  "packageManager": "yarn@4.10.3"
}
